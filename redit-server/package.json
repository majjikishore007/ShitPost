  "description": "a node backend for redit",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start2": "ts-node src/index.ts",
    "dev": "nodemon dist/index.js",
    "dev2": "nodemon --exec ts-node src/index.ts",
    "watch": "tsc -w",
