    "argon2": "^0.28.3",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "pg": "^8.7.1",
    "redis": "^4.0.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
